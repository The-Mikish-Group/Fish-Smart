# Fish-Smart Environment Variables Template
# Copy this file to .env and fill in your actual values

#==============================================================================
# Database Configuration
#==============================================================================
DB_SERVER_FISH_SMART=your-database-server.com
DB_USER_FISH_SMART=your-database-username
DB_PASSWORD_FISH_SMART=your-database-password
DB_NAME_FISH_SMART=your-database-name

#==============================================================================
# Application Configuration
#==============================================================================
ADMIN_EMAIL_FISH_SMART=admin@yourdomain.com
ADMIN_PASSWORD_FISH_SMART=YourSecureAdminPassword123!
SYNCFUSION_KEY=your-syncfusion-license-key
DEFAULT_CITY_FISH_SMART=Your City
DEFAULT_STATE_FISH_SMART=Your State
DEFAULT_ZIPCODE_FISH_SMART=12345

#==============================================================================
# Site Information
#==============================================================================
SITE_URL_FISH_SMART=https://yourdomain.com
SITE_NAME_FISH_SMART=Fish-Smart
SITE_DOMAIN_FISH_SMART=yourdomain.com
SITE_ORG=Your Organization

#==============================================================================
# SMTP Configuration
#==============================================================================
SMTP_SERVER_FISH_SMART=your-smtp-server.com
SMTP_USERNAME_FISH_SMART=your-smtp-username
SMTP_PASSWORD_FISH_SMART=your-smtp-password
SMTP_PORT=587
SMTP_SSL=true

#==============================================================================
# Container-Specific Settings (usually don't need to change)
#==============================================================================
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://+:8080